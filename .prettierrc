{
    "trailingComma": "all",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "**/*.scss",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "**/*.css",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "**/*.less",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "**/*.ts",
            "options": {
                "tabWidth": 2
            }
        },
        {
            "files": "**/*.tsx",
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
